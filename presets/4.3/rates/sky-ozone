rateprofile 0

# rateprofile 0
set rates_type = BETAFLIGHT
set roll_rc_rate = 127
set pitch_rc_rate = 127
set yaw_rc_rate = 100
set roll_expo = 50
set pitch_expo = 50
set yaw_expo = 15
set roll_srate = 72
set pitch_srate = 72
set yaw_srate = 75

rateprofile 1

# rateprofile 1
set rates_type = BETAFLIGHT
set roll_rc_rate = 104
set pitch_rc_rate = 110
set yaw_rc_rate = 101
set roll_srate = 68
set pitch_srate = 68
set yaw_srate = 68

rateprofile 2

# rateprofile 2
set rates_type = BETAFLIGHT
set roll_rc_rate = 117
set pitch_rc_rate = 117
set yaw_rc_rate = 100
set roll_expo = 50
set pitch_expo = 50
set yaw_expo = 25
